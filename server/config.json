{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": true,
      "normalizeHttpPath": false,
      "xml": true
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": false,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "debug": true
    }
  }
}
